<?xml version="1.0" ?>
<odoo>

    <act_window id="action_delivery_menu" name="Delivery" res_model="delivery.model"></act_window>
    <record id="view_list_delivery" model="ir.ui.view">
        <field name="name">Delivery Note List</field>
        <field name="model">delivery.model</field>
        <field name="arch" type="xml">
            <tree>
                <field name="number"></field>
                <field name="partner"/>
                <field name="products"/>
                <field name="total"/>
                <field name="date"/>
            </tree>
        </field>
    </record>
</odoo>